{
  "name": "suprematism-style-guide",
  "version": "0.0.2",
  "description": "A style guide based on ConciseCSS.",
  "main": "src/docs/index.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CINBCUniversal/suprematism-style-guide.git"
  },
  "keywords": [],
  "author": "Zack Fantauzzi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CINBCUniversal/suprematism-style-guide/issues"
  },
  "scripts": {
    "build": "npm run build:icons && npm run build:guide && npm run build:docs",
    "build:watch": "npm run build -- watch",
    "build:guide": "node scripts/build-style-guide.js",
    "build:icons": "node scripts/parse-icons-demo.js",
    "build:docs": "ng build --bh /suprematism-style-guide/",
    "clean": "rm -rf node_modules && rm package-lock.json && npm cache verify",
    "ng": "ng",
    "start": "ng serve",
    "test": "npm run prettier-test && npm run lint && npm run test:unit && npm run test:e2e",
    "test:ci": "npm run prettier-test && npm run lint && npm run test:unit:ci && npm run test:e2e:ci",
    "test:unit": "ng test --watch=false",
    "test:unit:ci": "karma start karma.sauce.conf.js",
    "test:e2e": "ng e2e",
    "pretest:e2e:ci": "webdriver-manager update --versions.chrome=2.29",
    "test:e2e:ci": "ng e2e --progress false --config protractor.sauce.conf.js --webdriver-update false",
    "lint": "ng lint",
    "_prettier": "prettier --print-width 80 --tab-width 2 --single-quote --trailing-comma none",
    "prettier": "npm run _prettier -- --write  \"src/**/*.ts\"",
    "prettier-test": "npm run _prettier -- -l \"src/**/*.ts\"",
    "commit": "transform-index-and-commit 'npm run _prettier -- --write'",
    "prepush": "assert-on-head 'npm run test'"
  },
  "lint-staged": {
    "*.ts": [
      "_prettier-test"
    ]
  },
  "homepage": "https://cinbcuniversal.github.io/suprematism-style-guide/index.html",
  "dependencies": {
    "@angular/common": "^4.3.3",
    "@angular/compiler": "^4.3.3",
    "@angular/core": "^4.3.3",
    "@angular/forms": "^4.3.3",
    "@angular/http": "^4.3.3",
    "@angular/platform-browser": "^4.3.3",
    "@angular/platform-browser-dynamic": "^4.3.3",
    "@angular/router": "^4.3.3",
    "ag-grid": "^10.1.0",
    "ag-grid-angular": "^10.1.0",
    "ag-grid-enterprise": "^10.1.0",
    "core-js": "^2.5.0",
    "prismjs": "^1.6.0",
    "rxjs": "^5.4.1",
    "shave": "^2.0.4",
    "suprematism-badge": "github:cinbcuniversal/suprematism-badge",
    "suprematism-bullet-chart": "github:cinbcuniversal/suprematism-bullet-chart",
    "suprematism-clearable-input": "github:cinbcuniversal/suprematism-clearable-input",
    "suprematism-donut-chart": "github:cinbcuniversal/suprematism-donut-chart",
    "suprematism-dropdown-menu": "github:cinbcuniversal/suprematism-dropdown-menu",
    "suprematism-dropdown-select": "github:cinbcuniversal/suprematism-dropdown-select",
    "suprematism-font-icons": "github:cinbcuniversal/suprematism-font-icons",
    "suprematism-modal": "github:cinbcuniversal/suprematism-modal",
    "suprematism-multi-select-search": "github:cinbcuniversal/suprematism-multi-select-search",
    "suprematism-popover": "github:cinbcuniversal/suprematism-popover",
    "suprematism-range-input": "github:cinbcuniversal/suprematism-range-input",
    "suprematism-select-search": "github:cinbcuniversal/suprematism-select-search",
    "suprematism-toggle-switch": "github:cinbcuniversal/suprematism-toggle-switch",
    "suprematism-truncate-text": "github:cinbcuniversal/suprematism-truncate-text",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.8.16"
  },
  "devDependencies": {
    "@angular/cli": "^1.3.0",
    "@angular/compiler-cli": "^4.3.3",
    "@types/jasmine": "^2.5.53",
    "@types/node": "^8.0.23",
    "@types/prismjs": "^1.4.18",
    "basscss": "^8.0.3",
    "cheerio": "^1.0.0-rc.2",
    "codelyzer": "^3.1.2",
    "concat-files": "^0.1.1",
    "concise-cli": "^0.4.1",
    "concise-ui": "^0.2.0",
    "concise-utils": "^1.0.0",
    "concise.css": "^4.0.1",
    "fs-extra": "^4.0.1",
    "git-quality": "github:CINBCUniversal/git-quality",
    "husky": "^0.14.3",
    "jasmine-core": "^2.7.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-sauce-launcher": "^1.1.0",
    "lint-staged": "^4.0.3",
    "node-sass": "^4.5.3",
    "node-watch": "^0.5.5",
    "prettier": "^1.4.4",
    "protractor": "~5.1.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.6.0",
    "typescript": "~2.2.0"
  }
}
