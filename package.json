{
  "name": "suprematism-style-guide",
  "version": "0.0.2",
  "description": "A style guide based on ConciseCSS.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CINBCUniversal/suprematism-style-guide.git"
  },
  "keywords": [],
