  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CINBCUniversal/suprematism-style-guide/issues"
  },
  "scripts": {
    "build": "npm cache clean && npm install && npm run build:icons && npm run build:guide && npm run build:docs",
    "build:watch": "npm run build -- watch",
    "build:guide": "node lib/build-style-guide.js",
    "build:icons": "node lib/parse-icons-demo.js",
    "build:docs": "cd docs-src && npm run build && cd ../",
    "start": "cd docs-src && npm run start && cd ../"
  },
  "homepage": "https://cinbcuniversal.github.io/suprematism-style-guide/index.html",
  "dependencies": {},
  "devDependencies": {
    "basscss": "^8.0.3",
    "cheerio": "^0.22.0",
    "concat-files": "^0.1.1",
    "concise-cli": "^0.4.1",
    "concise-ui": "^0.2.0",
    "concise-utils": "^1.0.0",
    "concise.css": "^4.0.1",
    "fs-extra": "^2.0.0",
    "node-sass": "^3.10.1",
    "node-watch": "^0.4.1",
    "suprematism-font-icons": "github:cinbcuniversal/suprematism-font-icons"
  }
}
